	<script>

		jQuery(document).ready(function($) {



		$(".style-scrollingstrip").flickity({
		  // options
		  /*
		  cellAlign: 'left',
		  contain: true,
		  pageDots: false
		  */

		  cellAlign: 'left',
		  contain: true,
		  pageDots: false,
		  setGallerySize: false,
		  imagesLoaded: true
		});



		$(window).on('orientationchange', function() {
    document.body.style.display='none';
    document.body.offsetHeight; //cause a reflow
    document.body.style.display='block'; //cause a repaint
});

/*
			$(window).on( 'orientationchange', forceRedraw ); // shouldn't have had parentheses

			function forceRedraw(){
				document.body.style.display='none';
    			document.body.offsetHeight; //cause a reflow
    			document.body.style.display='block'; //cause a repaint
			}
		*/
		
			// Rely on CSS for all the overlay work:

			function closeOverlay(){
				$("body").removeClass("overlaidgallery");
				$(".gallery-overlay").removeClass("active");
			}

			function openOverlay(gal){

					$("body").addClass("overlaidgallery");
					$("." + gal).addClass("active");
					// $("body").scrollTop( 0 ); // only if we hide the page behind

			}


			// Clicking the overlay dismisses the carousel, so block any clicks on the
			// active carousel area except the close button:

			$(".gallery-overlay").click(closeOverlay);
			$(".owl-carousel").click(function( event ) {
				event.stopPropagation();
			});
			$(".close").click(closeOverlay);


			$(".owl-carousel").owlCarousel({
			navigation : true,
			navigationText : ["&#xf104;","&#xf105;"],
			pagination : false,
			//paginationNumbers : true,
			singleItem: true
		});

			$( "body" ).on( "keydown", function( e ) {
				var code = e.keyCode;
				var owlkeys = $(".gallery-overlay.active .owl-carousel").data('owlCarousel');
				if( code == 39 ) {
					//$next.trigger( "click" );
					owlkeys.next();
				}
				if( code == 37 ) {
					//$previous.trigger( "click" );
					owlkeys.prev();
				}
				if( code == 27 ) {
					//$previous.trigger( "click" );
					closeOverlay();
				}

			});


			// Clicking a thumbnail advances the carousel to the right large image:

			$(".gallery-item").click(function() {

			var galleryID = $(this).closest(".gallery").attr('id');
			// changed parent to closest as flickity on the strip style inserts extra layers of divs
			openOverlay( galleryID );
			var owl = $("." + galleryID + " .owl-carousel").data('owlCarousel');
			var slidenum = $(this).parent().children().index(this);
			//var slidenum = $(this).index();
    		owl.jumpTo(slidenum);
		});

			objectFitImages();

		}); // jQuery(document).ready(function($)
	</script>

<script src="https://www.pulteneybridgeflowers.co.uk/wp-content/themes/pulteney/js/navigation.js"></script>
<script>
jQuery(document).ready(function($) {
(function($,sr){

  // debouncing function from John Hann
  // http://unscriptable.com/index.php/2009/03/20/debouncing-javascript-methods/
  var debounce = function (func, threshold, execAsap) {
      var timeout;

      return function debounced () {
          var obj = this, args = arguments;
          function delayed () {
              if (!execAsap)
                  func.apply(obj, args);
              timeout = null;
          };

          if (timeout)
              clearTimeout(timeout);
          else if (execAsap)
              func.apply(obj, args);

          timeout = setTimeout(delayed, threshold || 100);
      };
  }
  // smartresize 
  jQuery.fn[sr] = function(fn){  return fn ? this.bind('resize', debounce(fn)) : this.trigger(sr); };

})(jQuery,'smartresize');

causeRepaintsOn = $("body");

// usage:
$(window).smartresize(function(){
 causeRepaintsOn.css("z-index", 1);
});
});
</script>
</body>
